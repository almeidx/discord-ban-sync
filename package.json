{
	"name": "discord-ban-sync",
	"author": "Almeida <almeidx@pm.me> (almeidx.dev)",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "rimraf dist && tsc",
		"lint": "prettier --check . && eslint src --ext ts",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"start": "yarn build && node --enable-source-maps dist/index.js",
		"update": "yarn upgrade-interactive"
	},
	"dependencies": {
		"discord.js": "14.6.0",
		"dotenv": "^16.0.3",
		"pino": "^8.7.0",
		"pino-pretty": "^9.1.1",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@types/node": "^16.18.0",
		"eslint": "^8.26.0",
		"eslint-config-neon": "^0.1.39",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"typescript": "~4.8.4"
	},
	"packageManager": "yarn@3.2.4"
}
